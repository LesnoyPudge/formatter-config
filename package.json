{
    "name": "@lesnoypudge/formatter-config",
    "version": "6.1.0",
    "description": "lesnoypudge's formatter-config",
    "main": "./build/index.js",
    "type": "module",
    "bin": {
        "formatter-config": "./build/index.js"
    },
    "scripts": {
        "dev": "node --experimental-strip-types ./src/index.ts",
        "deploy": "git add . && git commit -m 'update' && git push && git status",
        "publish": "npm publish --access public",
        "build": "builder --file tsconfig.node.build.json"
    },
    "keywords": [
        "prettier",
        "lesnoypudge",
        "formatter",
        "config"
    ],
    "author": "LesnoyPudge",
    "license": "ISC",
    "devDependencies": {
        "@lesnoypudge/builder": "^2.0.0",
        "@lesnoypudge/types-global-base": "^4.0.0",
        "@lesnoypudge/types-global-node": "^1.0.0"
    },
    "dependencies": {
        "prettier": "^3.3.3"
    }
}